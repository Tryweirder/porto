#!/bin/sh
# prerm script
echo "!!! prerm $1"
case "$1" in
    purge|remove)
    if [ -e "/etc/init/yandex-porto.conf" ]; then
        stop yandex-porto || true
    fi
    ;;
    upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
    ;;
    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

exit 0
